{
  "name": "gamebryo-plugin-management",
  "version": "0.2.1",
  "description": "Management for gamebryo plugins",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "webpack": "webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "postinstall": "electron-rebuild -w esptk,loot -t dev,prod",
    "copy_esptk": "copyfiles -f ./node_modules/esptk/build/Release/nbind.node ./dist/esptk",
    "copy_loot": "copyfiles -f ./node_modules/loot/build/Release/nbind.node ./dist/loot && copyfiles -f ./node_modules/loot/loot_api/loot_api.dll ./dist/loot",
    "copy_async": "copyfiles -f ./node_modules/loot/async.js ./dist && copyfiles -f ./node_modules/loot/bindings.js ./dist",
    "build": "npm run webpack && npm run copy_esptk && npm run copy_loot && npm run copy_async && copyfiles -f src/stylesheets/plugin_management.scss dist && extractInfo",
    "installDev": "copyfiles -u 1 dist/**/* ../../out/bundledPlugins/gamebryo-plugin-management",
    "buildDev": "npm run build && npm run installDev"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@types/cytoscape": "3.2.7",
    "@types/i18next": "^8.4.4",
    "@types/iconv-lite": "^0.0.1",
    "@types/immutability-helper": "^2.6.3",
    "@types/js-yaml": "^3.11.2",
    "@types/react": "^16.4.11",
    "@types/react-bootstrap": "0.32.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-faux-dom": "^4.1.1",
    "@types/react-i18next": "^7.8.2",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-select": "^1.2.1",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "copyfiles": "^2.0.0",
    "cytoscape": "^3.2.16",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-edgehandles": "^3.4.0",
    "electron-rebuild": "^1.8.2",
    "esptk": "Nexus-Mods/node-esptk",
    "i18next": "^11.6.0",
    "iconv-lite": "^0.4.23",
    "immutability-helper": "^2.7.1",
    "js-yaml": "^3.12.0",
    "json-loader": "^0.5.7",
    "loot": "Nexus-Mods/node-loot",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "ts-loader": "4.4.2",
    "vortex-api": "Nexus-Mods/vortex-api",
    "webpack": "^4.16.5"
  }
}
